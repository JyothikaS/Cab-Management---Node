<!--navbar ends-->
<h1 class="text-center">Registration Details</h1>
<br>
<div class="container">
    {{!-- <a href="/cab/create" class="btn btn-primary">+ Add Cab</a> --}}
    <table class="table table-striped mt-4">
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Contact</th>
            <th>Address</th>
            <th>DOB</th>
            <th>Gender</th>
            <th>Password</th>
            <th>Action</th>
        </tr>
        {{#each data}}
        <tr>
            <td>{{this.dataValues.id}}</td>
            <td>{{this.dataValues.firstName}}</td>
            <td>{{this.dataValues.lastName}}</td>
            <td>{{this.dataValues.email}}</td>
            <td>{{this.dataValues.phone}}</td>
            <td>{{this.dataValues.address}}</td>
            <td>{{this.dataValues.dob}}</td>
            <td>{{this.dataValues.gender}}</td>
            <td>{{this.dataValues.password}}</td>
            <td>
                <a href="/cab/update/{{this.dataValues.id}}">Edit</a> |
                <a href="/cab/delete/{{this.dataValues.id}}" class="text-danger"
                    onclick="return confirm('Are you sure you want to delete this')">Delete</a>
            </td>
        </tr>
        {{/each}}
    </table>
</div>
<!--footer-->
<div class="footer">
    <img src="https://quick-cabs.com/images/footer.png" class="img-fluid" alt="">
</div>
